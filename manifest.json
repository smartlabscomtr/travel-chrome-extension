{
  "name": "Inspiration Tab",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Beautifull photos + amazing flight tickets prices",
  "icons": {
    "128": "img/icon128.png"
  },
  "background": {
    "page": "bg.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://api.keen.io; object-src 'self'",
  "permissions": [
    "alarms",
    "storage"
  ]
}